{
  "name": "nxus-clientjs",
  "version": "4.0.0-6",
  "description": "Client JS compilation for Nxus applications",
  "main": "lib/",
  "scripts": {
    "preversion": "npm test",
    "postversion": "npm run build-docs && git push && git push --tags",
    "test": "NODE_ENV=test mocha --recursive --compilers js:babel-register -R spec src/**/*",
    "compile": "babel src --out-dir lib",
    "prepublish": "npm run compile",
    "postpublish": "npm run build-docs && npm run publish-docs",
    "build-docs": "export NAME=`npm view . name`; export VERSION=`npm view . version`; documentation readme ./src/index.js --name $NAME --project-version $VERSION --readme-file ./README.md -s $NAME",
    "publish-docs": "npm run build-docs && git add ./README.md && git commit -m 'Updated README API Docs' && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nxus/clientjs.git"
  },
  "keywords": [
    "nxus",
    "client",
    "browserify"
  ],
  "author": "mjreich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nxus/clientjs/issues"
  },
  "homepage": "https://github.com/nxus/clientjs",
  "dependencies": {
    "babelify": "^7.3.0",
    "bluebird": "^3.0.6",
    "browserify": "^12.0.1",
    "crisper": "^2.0.2",
    "fs-extra": "^0.30.0",
    "minifyify": "^7.3.3",
    "morph": "^0.2.0",
    "nxus-core": "^4.0.0-3",
    "nxus-router": "^4.0.0-0",
    "nxus-templater": "^4.0.0-0",
    "rc": "^1.1.6",
    "traverse": "^0.6.6",
    "underscore": "^1.8.3",
    "vulcanize": "^1.14.8",
    "watchify": "^3.6.1"
  },
  "devDependencies": {
    "documentation": "^4.0.0-beta9",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "mocha": "^2.2.5",
    "should": "^7.0.2",
    "sinon": "^1.17.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-function-bind": "^6.8.0"
  }
}
